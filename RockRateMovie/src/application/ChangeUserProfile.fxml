            <Font name="Arial Black" size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
        <Button layoutX="540.0" layoutY="640.0" mnemonicParsing="false" onAction="#backOnAction" prefHeight="38.0" prefWidth="226.0" text="Back">
         <font>
            <Font name="Arial Black" size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Label fx:id="LabelText" alignment="CENTER" layoutX="455.0" layoutY="510.0" prefHeight="29.0" prefWidth="371.0" textFill="#f51d1d">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
